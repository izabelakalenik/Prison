{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "Users",
    "properties": {
        "username": {
            "type": "string"
        },
        "password": {
            "type": "string"
        },
        "user_id": {
            "type": "string",
            "pattern": "^[a-fA-F0-9]{24}$"
        },
        "user_type": {
            "type": "string"
        }
    },
    "additionalProperties": false
}