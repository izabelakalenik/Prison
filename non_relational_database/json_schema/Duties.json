{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "Duties",
    "properties": {
        "_id": {
            "type": "string",
            "pattern": "^[a-fA-F0-9]{24}$"
        },
        "id_block": {
            "type": "string",
            "pattern": "^[a-fA-F0-9]{24}$"
        },
        "ids_guards": {
            "type": "array",
            "items": {
                "type": "string",
                "pattern": "^[a-fA-F0-9]{24}$"
            }
        },
        "start_date": {
            "type": "string",
            "format": "date-time"
        },
        "end_date": {
            "type": "string",
            "format": "date-time"
        }
    },
    "additionalProperties": false
}